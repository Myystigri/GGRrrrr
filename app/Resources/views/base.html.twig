<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome to GGRrrrr !{% endblock %}</title>

        {% block stylesheets %}
            {% stylesheets 'bundles/app/css/*' filter='cssrewrite' %}
                <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}

    </head>
    <body>

        {% block javascripts %}
            <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
            <script type='text/javascript' src='https://maps.googleapis.com/maps/api/js?key=AIzaSyBasoY5XOYA82Kgb26TJGqhiTR8l2sh0ZY&sensor=false&libraries=geometry'></script>
            {% javascripts '@AppBundle/Resources/public/js/*' %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}

        {% block body %}{% endblock %}
    </body>
</html>
